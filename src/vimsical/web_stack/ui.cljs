(ns vimsical.web-stack.ui
  (:refer-clojure :exclude [list comment])
  (:require [reagent.core :as reagent]
            ["semantic-ui-react" :as sem]))

(defn adapt-component-fn
  "Get a component from a react component lib, like sematic-ui-react:
    (def component (adapt-component-fn semantic-ui))
    (def sem-btn (component \"Button\"))"
  [obj]
  (fn [k & ks]
    (reagent/adapt-react-class
     (if (seq ks)
       (apply goog.object/getValueByKeys obj k ks)
       (goog.object/get obj k)))))

(defonce component (adapt-component-fn sem))

